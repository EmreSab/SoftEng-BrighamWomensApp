<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox styleClass="background" prefWidth="262.0" spacing="5.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.FlashyFrogs.MapEditor.ExitConfirmationController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="bodyTextObject" text="Changes you made may not be saved!" />
      <Button fx:id="continueEditing" alignment="BASELINE_LEFT" cancelButton="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="240.0" styleClass="blueOutlineNoSetSize" text="Continue Editing">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="discard" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="26.0" prefWidth="240.0" styleClass="redOutlineNoSetSize" text="Discard Changes and Exit">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="save" alignment="BASELINE_LEFT" defaultButton="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="240.0" styleClass="blueNoSetSize" text="Save Changes and Exit">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
