<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.FlashyFrogs.MapEditor.ExitConfirmationController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="titleText" text="Confirm Exit?" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="headerBold" text="Changes you made may not be saved!" />
      <Button fx:id="continueEditing" cancelButton="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="204.0" text="Continue Editing" styleClass="no_background_black">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="left-alight" />
            <String fx:value="round" />
            <String fx:value="blueOutline" />
            <String fx:value="body" />
         </styleClass>
      </Button>
      <Button fx:id="discard" mnemonicParsing="false" prefHeight="26.0" prefWidth="204.0" text="Discard Changes and Exit">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="b2"/>
            <String fx:value="left-alight"/>
         </styleClass>
      </Button>
      <Button fx:id="save" mnemonicParsing="false" prefHeight="26.0" prefWidth="204.0" text="Save Changes and Exit">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="blue3" />
            <String fx:value="body" />
            <String fx:value="round" />
            <String fx:value="left-alight"/>
         </styleClass>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
